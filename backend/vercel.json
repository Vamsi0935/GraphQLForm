{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/graphql",
        "destination": "http://localhost:5000/graphql"
      }
    ]      
}
  